{
  "name": "@example/astro-clean",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build && npm run format:dist",
    "build:clean": "astro build && npm run format:dist && npm run postbuild",
    "format:dist": "prettier --write \"dist/**/*.{html,css,js}\" --ignore-path .prettierignore",
    "format:src": "prettier --write \"src/**/*.{astro,ts,js,scss}\"",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "astro": "^4.15.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.0",
    "typescript": "^5.0.0",
    "sass": "^1.69.0",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.0"
  }
}