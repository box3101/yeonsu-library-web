{
  "name": "@example/basics",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev --host",
    "dev:quiet": "astro dev --host --silent",
    "dev:turbo": "concurrently \"npm run dev\" \"npm run watch:scss\"",
    "dev:full": "concurrently \"npm run dev\" \"npm run watch:scss\" \"npm run watch:files\"",
    "start": "npm run dev",
    "build": "astro build",
    "build:clean": "astro build",
    "prettier": "prettier --write \"**/*.html\"",
    "build:jsp": "astro build && npm run remove-astro-attrs && npm run format:dist",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "format:src": "prettier --write \"src/**/*.{astro,ts,js,scss}\"",
    "format:dist": "prettier --write \"dist/**/*.{css,js,html}\"",
    "watch:scss": "concurrently \"npm run watch:scss:base\" \"npm run watch:scss:utilities\" \"npm run watch:scss:header\" \"npm run watch:scss:footer\" \"npm run watch:scss:sub\" \"npm run watch:scss:components\" \"npm run watch:scss:home\"",
    "watch:scss:base": "sass --watch src/styles/base.scss:public/assets/css/base.css --source-map",
    "watch:scss:utilities": "sass --watch src/styles/utilities.scss:public/assets/css/utilities.css --source-map",
    "watch:scss:header": "sass --watch src/styles/header.scss:public/assets/css/header.css --source-map",
    "watch:scss:footer": "sass --watch src/styles/footer.scss:public/assets/css/footer.css --source-map",
    "watch:scss:sub": "sass --watch src/styles/sub.scss:public/assets/css/sub.css --source-map",
    "watch:scss:components": "sass --watch src/styles/components.scss:public/assets/css/components.css --source-map",
    "watch:scss:home": "sass --watch src/styles/home.scss:public/assets/css/home.css --source-map",
    "build:scss": "npm run build:scss:base && npm run build:scss:utilities && npm run build:scss:header && npm run build:scss:footer && npm run build:scss:sub && npm run build:scss:components && npm run build:scss:home",
    "build:scss:base": "sass src/styles/base.scss:public/assets/css/base.css --no-source-map",
    "build:scss:utilities": "sass src/styles/utilities.scss:public/assets/css/utilities.css --no-source-map",
    "build:scss:header": "sass src/styles/header.scss:public/assets/css/header.css --no-source-map",
    "build:scss:footer": "sass src/styles/footer.scss:public/assets/css/footer.css --no-source-map",
    "build:scss:sub": "sass src/styles/sub.scss:public/assets/css/sub.css --no-source-map",
    "build:scss:components": "sass src/styles/components.scss:public/assets/css/components.css --no-source-map",
    "build:scss:home": "sass src/styles/home.scss:public/assets/css/home.css --no-source-map",
    "watch:files": "echo 'File watching not implemented'",
    "remove-astro-attrs": "node scripts/remove-astro-attrs.cjs",
    "update-guide": "echo 'Update guide script not implemented'",
    "setup-hooks": "echo 'Setup hooks script not implemented'",
    "pre-commit": "npm run update-guide",
    "sass": "sass --watch src/styles/main.scss:public/assets/css/main.css --no-source-map"
  },
  "dependencies": {
    "@astrojs/react": "^2.2.1",
    "@astrojs/renderer-react": "^0.5.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "better-sqlite3": "^12.2.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jquery": "^3.7.0",
    "method-override": "^3.0.0",
    "mongodb": "^3.7.4",
    "mongoose": "^7.3.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "astro": "^2.10.15",
    "concurrently": "^8.2.0",
    "glob": "^11.0.3",
    "js-beautify": "^1.15.4",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.8.0",
    "sass": "^1.64.1"
  },
  "description": "``` npm create astro@latest -- --template basics ```",
  "main": "app.js",
  "author": "",
  "license": "ISC"
}
