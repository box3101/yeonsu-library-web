{
  "name": "@example/basics",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev --host",
    "dev:quiet": "astro dev --host --silent",
    "dev:turbo": "concurrently \"npm run dev\" \"npm run watch:scss\"",
    "dev:full": "concurrently \"npm run dev\" \"npm run watch:scss\" \"npm run watch:files\"",
    "start": "npm run dev",
    "build": "astro build && npm run format:dist",
    "build:clean": "astro build && npm run format:dist",
    "build:jsp": "astro build && npm run remove-astro-attrs && npm run format:dist",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "format:src": "prettier --write \"src/**/*.{astro,ts,js,scss}\"",
    "format:dist": "prettier --write \"dist/**/*.{css,js}\" && js-beautify --type html --indent-with-tabs --end-with-newline --max-preserve-newlines 1 --wrap-line-length 150 --replace \"dist/**/*.html\"",
    "watch:scss": "sass --watch src/styles/main.scss:public/assets/css/main.css",
    "watch:files": "echo 'File watching not implemented'",
    "remove-astro-attrs": "node scripts/remove-astro-attrs.cjs",
    "update-guide": "echo 'Update guide script not implemented'",
    "setup-hooks": "echo 'Setup hooks script not implemented'",
    "pre-commit": "npm run update-guide",
    "prettier": "prettier --write \"**/*.html\" --ignore-path=.gitignore",
    "sass": "concurrently \"sass --watch src/styles/main.scss:public/assets/css/main.css\""
  },
  "dependencies": {
    "@astrojs/react": "^2.2.1",
    "@astrojs/renderer-react": "^0.5.0",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "astro": "^2.0.6",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jquery": "^3.7.0",
    "method-override": "^3.0.0",
    "mongodb": "^3.7.4",
    "mongoose": "^7.3.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "glob": "^11.0.3",
    "js-beautify": "^1.15.4",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "^1.64.1"
  },
  "description": "``` npm create astro@latest -- --template basics ```",
  "main": "app.js",
  "author": "",
  "license": "ISC"
}
